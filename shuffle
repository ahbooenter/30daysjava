function shuffle(array){
  let n = array.length-1
  console.log(n)
  let r1 = Math.ceil(Math.random()*n)
  let r2 = Math.ceil(Math.random()*n)
 
  console.log(r1,r2)
  let temp = array[r1]
  array[r1] = array[r2]
  array[r2] = temp
  return array
}
let x = [1,2,3,4,5]

let i = 0
while(i != 5){
  console.log(shuffle(x))
  i++
}
